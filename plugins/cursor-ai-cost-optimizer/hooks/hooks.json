{
  "version": 1,
  "hooks": {
    "sessionStart": [
      {
        "command": "node ./scripts/cco-session-start.mjs"
      }
    ],
    "beforeShellExecution": [
      {
        "command": "node ./scripts/cco-shell-guard.mjs",
        "matcher": ".*"
      }
    ],
    "sessionEnd": [
      {
        "command": "node ./scripts/cco-session-end.mjs"
      }
    ]
  }
}
